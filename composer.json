    "description": "Wunder's extendable template for DDEV Drupal projects.",
    "type": "composer-plugin",
    "license": "GPL-2.0-or-later",
    "homepage": "https://github.com/wunderio/ddev-drupal",
    "authors": [
        {
            "name": "Hannes Kirsman",
            "email": "hannes.kirsman@wunder.io",
            "homepage": "https://github.com/hkirsman",
            "role": "Developer"
        }
    ],
    "require": {
        "drupal/core": "^10.3 || ^11",
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
